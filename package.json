    "globals": "^15.0.0",
    "only-allow": "^1.1.1",
    "tslib": "^2.6.0",
    "types-mediawiki": "^1.9.1",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.18.2"
  },
  "pnpm": {
